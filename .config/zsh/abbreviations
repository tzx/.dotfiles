abbr "alog"="aws sso login --sso-session timmy-aws"
abbr "awsbuildlogs"="aws codebuild list-builds --region us-east-1 --max-items 5 | jq '.ids | .[]' | fzf | xargs aws codebuild batch-get-builds --ids | jq '.builds | first | .logs.groupName' | xargs awslogs get"
abbr "br"="git branch -vv --sort=-committerdate | fzf --height=50% --reverse --info=inline | awk '{print \$1;}' | xargs git switch"
abbr "dotfiles"="git --git-dir=\$HOME/.dotfiles/ --work-tree=\$HOME"
abbr "e"="nvim"
abbr "fe"="fzf | xargs nvim"
abbr "gau"="git add -u"
abbr "gcm"="git commit -m"
abbr "gd"="git diff HEAD"
abbr "gdt"="git -c diff.external=difft diff"
abbr "glog"="gcloud auth login && gcloud auth application-default login"
abbr "gss"="git status --short"
abbr "ll"="eza -la"
abbr "ls"="eza"
abbr "switch_me_aws"="export AWS_PROFILE=\$(aws configure list-profiles | fzf)"
